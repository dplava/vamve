   <dcterms:title>Visit of Queen Kapi'olani and Princess Lili-uokalani to Mount Vernon</dcterms:title>
   <dcterms:creator>Dillon, Luke C., 1836-1904</dcterms:creator>
   <dcterms:contributor>Mount Vernon Ladies' Association</dcterms:contributor>
